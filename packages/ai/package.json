{
  "name": "@webstudio-is/ai",
  "version": "0.1.0",
  "description": "AI tools",
  "author": "Webstudio <github@webstudio.is>",
  "homepage": "https://webstudio.is",
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "checks": "pnpm typecheck && pnpm lint",
    "dev": "build-package --watch",
    "build": "pnpm build:prompts && build-package",
    "build:twstyles": "tsx ./bin/twstyles.ts",
    "build:prompts": "tsx ./bin/prompts.ts \"./src/(chains|templates)/**/*.prompt.md\"",
    "dts": "tsc --declarationDir lib/types",
    "lint": "eslint ./src --ext .ts,.tsx --max-warnings 0",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "devDependencies": {
    "@webstudio-is/jest-config": "workspace:^",
    "@webstudio-is/scripts": "workspace:^",
    "@webstudio-is/tsconfig": "workspace:^",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "jest": "^29.3.1",
    "tsx": "^3.12.6",
    "type-fest": "^3.7.1",
    "typescript": "5.0.3"
  },
  "exports": {
    "source": "./src/index.ts",
    "import": "./lib/index.js",
    "require": "./lib/cjs/index.js"
  },
  "types": "lib/types/index.d.ts",
  "files": [
    "lib/*",
    "src/*",
    "!*.test.*"
  ],
  "license": "MIT",
  "private": false,
  "sideEffects": false,
  "dependencies": {
    "@builder.io/mitosis": "^0.0.101",
    "@webstudio-is/css-data": "workspace:^",
    "@webstudio-is/project": "workspace:^",
    "@webstudio-is/project-build": "workspace:^",
    "@webstudio-is/react-sdk": "workspace:^",
    "langchain": "^0.0.78",
    "mdast-util-from-markdown": "^1.3.0",
    "openai": "^3.2.1",
    "postcss": "^8.4.24",
    "postcss-custom-properties": "^13.1.5",
    "unist-util-visit": "^4.1.2"
  }
}
